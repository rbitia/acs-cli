{  
  "id": "/elkstack",
  "cmd": null,
  "cpus": 1,
  "mem": 3000,
  "disk": 0,
  "instances": 1,
  "constraints": [
    [
      "hostname",
      "UNIQUE"
    ]
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "rbitia/elk",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "$AGENT_FQDN",
    "HAPROXY_0_MODE": "http"
  },
  "portDefinitions": [
    {
      "port": 10000,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 10001,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 10002,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 10003,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 5601,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 9200,
      "protocol": "tcp",
      "labels": {}
    }
  ],
  "requirePorts": true
}
